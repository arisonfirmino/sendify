generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  lastName   String
  username   String   @unique
  email      String   @unique
  password   String
  created_at DateTime @default(now())

  @@map("users")
}
